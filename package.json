{
  "name": "angular2-semantic-ui",
  "version": "0.0.0",
  "description": "An Angular 2 components hub using Semantic UI frontend framework",
  "main": "",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "express": "node ./examples/server/express-server-example.js",
    "express-install": "npm install express connect-history-api-fallback morgan body-parser cors --save",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "e2e": "protractor",
    "ci": "npm run e2e && npm run test",
    "test": "karma start",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdo2/angular2-semantic-ui.git"
  },
  "author": "Francisco Javier Arribas Fern√°ndez <arribasjavier@gmail.com> (https://github.com/mdo2)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdo2/angular2-semantic-ui/issues"
  },
  "homepage": "https://github.com/mdo2/angular2-semantic-ui.git",
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.34.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "semantic-ui": "^2.1.6",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.3.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
